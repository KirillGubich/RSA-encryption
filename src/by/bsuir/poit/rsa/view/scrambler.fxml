<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.bsuir.poit.rsa.controller.ScramblerController">
   <children>
      <MenuBar layoutY="1.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Key">
            <items>
              <MenuItem fx:id="keyCalculateMenu" mnemonicParsing="false" onAction="#viewKeyCalculatorWindow" text="Calculate" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
